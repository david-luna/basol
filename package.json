{
  "name": "basol",
  "version": "0.5.0",
  "type": "module",
  "description": "Basic Observable Library",
  "types": "./lib/types.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "engines": {
    "node": ">=14.5.0"
  },
  "scripts": {
    "test": "node --test",
    "test:lcov": "node --test --experimental-test-coverage --test-reporter=lcov",
    "test:junit": "node --test --experimental-test-coverage --test-reporter=junit",
    "lcov": "cat lcov.info | coveralls",
    "lint": "npm run lint:js && npm run lint:ts",
    "lint:js": "eslint 'lib/**/*.js'",
    "lint:ts": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/david-luna/basol.git"
  },
  "keywords": [
    "observable",
    "utils",
    "library",
    "rxjs"
  ],
  "author": "David Luna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/david-luna/basol/issues"
  },
  "homepage": "https://github.com/david-luna/basol#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "coveralls": "^3.0.11",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1",
    "typescript": "^4.4.4"
  }
}
